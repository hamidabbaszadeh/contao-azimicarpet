{
    "name": "respinar/contao-azimicarpet",
    "description": "Carpets for Contao",
    "keywords": ["contao", "carpet"],
    "type": "contao-module",
    "homepage": "https://github.com/hamidabbaszadeh/contao-azimicarpet",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Hamid Abbaszadeh",
            "email": "info@respinar.com",
            "homepage": "http://respinar.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/hamidabbaszadeh/contao-azimicarpet/issues"
    },
    "require": {
        "php": ">=5.4",
        "contao/core-bundle":"~4.4",
        "contao-community-alliance/composer-plugin":"^2.4.1 || ~3.0"
    },
    "autoload": {
        "psr-4": {
            "Respinar\\Carpets\\": "src/library/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "src": "system/modules/azimicarpet"
            },
            "transifex": {
                "project": "contao-azimicarpet",
                "prefix": "master-",
                "languages_cto": "languages",
                "languages_tx": ".tx"
            }
        }
    }
}
